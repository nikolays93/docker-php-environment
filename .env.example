# Project name to separate multiple instances
DOCKER_NAME=default

# Project domain name (dont forget write to hosts after change)
APP_DOMAIN=localhost

# PHP
PHP_VERSION=7.4

# Database
DB_CONNECTION=mysql
# mysql57 | mysql8 | mysql8-arm | mariadb-arm
DB_VERSION=mysql8
DB_HOST=db
DB_PORT=3306
DB_DATABASE=app
DB_USERNAME=admin
DB_PASSWORD=admin
DB_ROOT_PASSWORD=admin
DB_PATH=./docker/data/db

# Network
NETWORK_SUBNET=172.20.0.0/24
NETWORK_INTERFACE=172.20.0.10
